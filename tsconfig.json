{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "solmate/*": ["solmate/src/*"]
    },
    "target": "es2019",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "outDir": "dist",
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "useUnknownInCatchVariables": false,
    "declaration": true
  },
  "include": ["./types", "./deployed", "./helpers", "./abis/**/*.json","./index.ts"]
}
